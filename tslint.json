{
    "extends": "tslint:recommended",
    "rules": {
      "curly": true,
      "quotemark": [true, "single"],
      "semicolon": [true, "never"],
      "trailing-comma": [true, "never"],
      "object-literal-sort-keys": false,
      "ordered-imports": [false],
      "variable-name": [true, "allow-leading-underscore"],
      "no-console": false
    }
  }